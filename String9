package String;

import java.util.Scanner;

public class String9 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Birinchi satrni kiriting: ");
        String satr1 = scanner.nextLine();
        System.out.print("Ikkinchi satrni kiriting: ");
        String satr2 = scanner.nextLine();

        String yangiSatr = satr1 + satr2;
        System.out.println("Yangi satr: " + yangiSatr);
    }
}
